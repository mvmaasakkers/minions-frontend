@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}


<div class="row">
    <div class="col-md-3">
        <h2>Application users</h2>
        <ul>
            @{ 
                foreach (var item in Model.Accounts)
                {
                  <LI><A href="Account?id=@item.Identifier">@item.Name</A></LI> 

                }
            }
        </ul>
    </div>
 </div>